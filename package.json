  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run check:consistency && npm run check:forbidden && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:ci": "jest --ci --coverage",
    "check:links": "node scripts/check-source-links.mjs",
    "check:forbidden": "node scripts/check-forbidden-words.js",
    "check:consistency": "node scripts/check-data-consistency.js",
    "check:all": "npm run check:consistency && npm run check:forbidden && npm run check:links",
    "monitor:links": "npx tsx src/scripts/link-monitor.ts",
    "test:e2e": "playwright test",
    "pages:build": "npx @cloudflare/next-on-pages@1",
    "pages:dev": "npm run pages:build && npx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat"
  },
  "dependencies": {
    "canvas-confetti": "^1.9.3",
    "clsx": "^2.1.1",
    "framer-motion": "^11.0.0",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.469.0",
    "next": "^14.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.15.4",
    "tailwind-merge": "^2.5.2"
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.6.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@cloudflare/next-on-pages": "^1.13.16",
    "wrangler": "^3.102.0",
    "@playwright/test": "^1.49.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.3.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.4.5"
  }
}
